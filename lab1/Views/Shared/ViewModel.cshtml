@model lab1.Models.MyModel

@{
    ViewData["Title"] = "ViewModel";
}
    <div>
        <h1 class="display-4">PassUsingModel</h1>
        <b>Rand Value First : </b> @Model.num1
        <br />
        <b>Rand Value Second : </b> @Model.num2
        <br />
        <b>Add : </b>
        <br />
        @Model.num1 + @Model.num2 = @Model.Add()
        <br />
        <b>Sub : </b>
        <br />
        @Model.num1 - @Model.num2 = @Model.Sub()
        <br />
        <b>Mult : </b>
        <br />
        @Model.num1 * @Model.num2 = @Model.Mult()
        <br />
        <b>Div : </b>
        <br />
        @Model.num1 / @Model.num2 = @Model.Div()
    </div>